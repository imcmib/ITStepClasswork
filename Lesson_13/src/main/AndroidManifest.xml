<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.itstep.android.classwork" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SimpleAlertDialogActivity"/>
        <activity android:name=".ActivityAlertDialogActivity"/>
        <activity android:name=".ListAlertDialogActivity"/>
        <activity android:name=".RadioAlertDialogActivity"/>
        <activity android:name=".CheckAlertDialogActivity"/>
        <activity android:name=".AutoCloseAlertDialogActivity"/>
        <activity android:name=".CustomAlertDialogActivity"/>
        <activity android:name=".DialogsActivity"/>
        <activity android:name=".DatePickerDialogActivity"/>
        <activity android:name=".TimePickerDialogActivity"/>
    </application>

</manifest>
